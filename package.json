  },
  "dependencies": {
    "@infisical/sdk": "^3.0.5",
    "@nestjs/common": "^8.0.0",
    "@nestjs/config": "^1.0.0",
    "@nestjs/core": "^8.4.7",
    "@nestjs/platform-express": "^8.0.0",
    "chokidar": "^4.0.3",
    "dotenv": "^10.0.0"
